{
  "name": "@go-starter-template/zod",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo node_modules"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "^9.2.1",
    "tsc-alias": "^1.8.16"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.8",
    "zod": "^4.1.5"
  }
}
