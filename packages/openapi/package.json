{
  "name": "@go-starter-template/openapi",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "gen": "tsx src/gen.ts",
    "build": "tsc && tsc-alias",
    "dev": "wait-on ../zod/dist/index.js && tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo node_modules"
  },
  "exports": {
    "./contracts": {
      "types": "./dist/contracts/index.d.ts",
      "default": "./dist/contract/index.js"
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.18.1",
    "concurrently": "^9.2.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.8",
    "@go-starter-template/zod": "workspace:*",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "ts-pattern": "^5.8.0",
    "zod": "^4.1.5"
  }
}
